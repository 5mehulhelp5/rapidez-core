{
    "name": "rapidez/core",
    "description": "Rapidez Core",
    "keywords": [
        "rapidez"
    ],
    "homepage": "https://rapidez.io",
    "authors": [
        {
            "name": "Roy Duineveld",
            "email": "royduineveld@gmail.com",
            "homepage": "https://royduineveld.nl",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "cviebrock/laravel-elasticsearch": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/events": "^8.0",
        "illuminate/queue": "^8.0",
        "illuminate/support": "^8.0",
        "tormjens/eventy": "^0.6"
    },
    "require-dev": {
        "orchestra/testbench": "^6.4",
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Rapidez\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rapidez\\Core\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapidez\\Core\\RapidezServiceProvider"
            ]
        }
    }
}
